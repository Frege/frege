--- frege.data.JSON depends on frege.data.Bits; importing frege.data.JSOn from frege.data.Bits leads to a circular dependency
module frege.data.BitSetJSON where

import frege.data.JSON (ToJSON, toJSON)
import frege.data.Bits (BitSet)

instance (ToJSON a, Enum a) => ToJSON (BitSet a) where
    toJSON = toJSON . BitSet.toList
