--- "Kind" of a case
module frege.compiler.enums.CaseKind where

import frege.data.JSON (ToJSON, toJSON)

{--
    case kind
 -}
data CKind =
      CNormal       --- normal case
    | CWhen         --- case that falls through
    | CNoWarn       --- compiler generated, do not emit warnings

derive Eq CKind
derive Enum CKind
derive Show CKind

instance ToJSON CKind where
  toJSON = toJSON . show