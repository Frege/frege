--- Names of name spaces.
module frege.compiler.types.NSNames where 


import frege.data.JSON (ToJSON, toJSON)

--- A namespace name, not to be confused with an ordinary 'String'
newtype NSName = NSX { unNS :: String }


derive Eq   NSName


derive Ord  NSName


derive Show NSName


instance ToJSON NSName where
  toJSON x = JSON.struct "NSX" $ JSON.struct "unNS" x.unNS
