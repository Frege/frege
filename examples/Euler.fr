fret a x = (a; x)
fbind :: (x -> (a,x)) -> (a -> x -> (b,x)) -> (x -> (b,x))
fbind !m k x = case m x of (!a, !x') -> k a x'
fthen !m1 m2 x = case m1 x of (_, !x') -> m2 x' 

